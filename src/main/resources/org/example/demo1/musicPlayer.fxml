<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="first" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo1.musicPlayer">
   <children>
      <Label fx:id="musicLabel" alignment="CENTER" prefHeight="50.0" prefWidth="600.0" text="Music">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <ProgressBar fx:id="progressBar" layoutY="61.0" prefHeight="25.0" prefWidth="600.0" progress="0.0" />
      <Button fx:id="nextbutton" layoutX="81.0" layoutY="99.0" mnemonicParsing="false" onAction="#nextMethod" prefHeight="38.0" prefWidth="52.0" text="Next">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="puseButton" layoutX="146.0" layoutY="99.0" mnemonicParsing="false" onAction="#puseMethod" prefHeight="38.0" prefWidth="52.0" text="Puse">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="play" layoutX="14.0" layoutY="99.0" mnemonicParsing="false" onAction="#playMethod" prefHeight="38.0" prefWidth="52.0" text="Play">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="216.0" layoutY="99.0" mnemonicParsing="false" onAction="#resetMethod" prefHeight="38.0" prefWidth="58.0" text="Reset">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="prevuseButton" layoutX="282.0" layoutY="99.0" mnemonicParsing="false" onAction="#prevuseMethod" prefHeight="38.0" prefWidth="58.0" text="Prevuse" />
      <ComboBox fx:id="speed" layoutX="354.0" layoutY="99.0" prefHeight="38.0" prefWidth="80.0" promptText="Speed" />
      <Slider fx:id="slider" layoutX="443.0" layoutY="111.0" max="200.0" prefHeight="14.0" prefWidth="153.0" value="100.0" />
   </children>
</AnchorPane>
